import requests
import datetime

# Function to make API request and log response
def make_api_request(api_url, log_file):
    # Make API request
    response = requests.get(api_url)
    
    # Log response and timestamp to file
    with open(log_file, 'a') as file:
        timestamp = datetime.datetime.now().strftime('%Y-%m-%d %H:%M:%S')
        file.write(f"{timestamp} - API: {api_url}, Response: {response.text}\n")

# List of APIs to make requests to
apis = [
    "https://api.example.com/endpoint1",
    "https://api.example.com/endpoint2",
    "https://api.example.com/endpoint3",
    "https://api.example.com/endpoint4",
    "https://api.example.com/endpoint5"
]

# File to log responses
log_file = "api_responses.log"

# Make API requests and log responses
for api_url in apis:
    make_api_request(api_url, log_file)
